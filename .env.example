# ===================================
# 银龄守候项目 - 环境变量配置模板
# 使用方法: 复制此文件为 .env 并填入真实值
# ===================================

# ===================================
# 基础配置
# ===================================
# 环境: dev/test/prod
ENVIRONMENT=dev

# 项目名称
PROJECT_NAME=yinling-guardian

# 项目版本
PROJECT_VERSION=1.0.0

# ===================================
# 数据库配置
# ===================================
# MySQL主库
DB_MASTER_HOST=localhost
DB_MASTER_PORT=3306
DB_MASTER_USERNAME=root
DB_MASTER_PASSWORD=your_mysql_password_here

# MySQL从库1
DB_SLAVE1_HOST=localhost
DB_SLAVE1_PORT=3307
DB_SLAVE1_USERNAME=root
DB_SLAVE1_PASSWORD=your_mysql_password_here

# MySQL从库2
DB_SLAVE2_HOST=localhost
DB_SLAVE2_PORT=3308
DB_SLAVE2_USERNAME=root
DB_SLAVE2_PASSWORD=your_mysql_password_here

# 数据库名称
DB_NAME_MAIN=yinling_main
DB_NAME_HEALTH=yinling_health
DB_NAME_MESSAGE=yinling_message
DB_NAME_SECURITY=yinling_security
DB_NAME_AI=yinling_ai

# ===================================
# Redis配置
# ===================================
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password_here
REDIS_DATABASE=0
REDIS_TIMEOUT=3000

# Redis集群配置 (生产环境)
REDIS_CLUSTER_NODES=localhost:7000,localhost:7001,localhost:7002,localhost:7003,localhost:7004,localhost:7005

# ===================================
# RabbitMQ配置
# ===================================
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USERNAME=admin
RABBITMQ_PASSWORD=your_rabbitmq_password_here
RABBITMQ_VIRTUAL_HOST=/

# RabbitMQ管理界面
RABBITMQ_MANAGEMENT_PORT=15672

# ===================================
# Nacos配置
# ===================================
NACOS_SERVER_ADDR=localhost:8848
NACOS_USERNAME=nacos
NACOS_PASSWORD=nacos
NACOS_NAMESPACE=dev

# ===================================
# 网关配置
# ===================================
GATEWAY_PORT=8080
GATEWAY_HOST=localhost

# ===================================
# 微服务端口配置
# ===================================
# 用户服务
USER_SERVICE_PORT=8081

# 健康管理服务
HEALTH_SERVICE_PORT=8082

# 消息通讯服务
MESSAGE_SERVICE_PORT=8083

# 安全监控服务
SECURITY_SERVICE_PORT=8084

# AI服务
AI_SERVICE_PORT=8085

# 定时任务服务
SCHEDULER_SERVICE_PORT=8086

# ===================================
# JWT配置
# ===================================
JWT_SECRET=your_jwt_secret_key_at_least_256_bits_here
JWT_EXPIRATION=86400000
JWT_REFRESH_EXPIRATION=604800000

# ===================================
# 讯飞开放平台 (ASR/TTS)
# ===================================
# 申请地址: https://www.xfyun.cn/
XUNFEI_APP_ID=your_xunfei_app_id_here
XUNFEI_API_KEY=your_xunfei_api_key_here
XUNFEI_API_SECRET=your_xunfei_api_secret_here

# 讯飞语音识别配置
XUNFEI_ASR_URL=wss://iat-api.xfyun.cn/v2/iat

# 讯飞语音合成配置
XUNFEI_TTS_URL=wss://tts-api.xfyun.cn/v2/tts

# ===================================
# 阿里云配置 (OCR/OSS/通义千问)
# ===================================
# 申请地址: https://www.aliyun.com/
ALIYUN_ACCESS_KEY_ID=your_aliyun_access_key_id_here
ALIYUN_ACCESS_KEY_SECRET=your_aliyun_access_key_secret_here
ALIYUN_REGION_ID=cn-hangzhou

# 阿里云OCR配置
ALIYUN_OCR_ENDPOINT=ocr.cn-hangzhou.aliyuncs.com

# 阿里云OSS配置
ALIYUN_OSS_ENDPOINT=oss-cn-hangzhou.aliyuncs.com
ALIYUN_OSS_BUCKET=yinling-guardian
ALIYUN_OSS_ACCESS_URL=https://yinling-guardian.oss-cn-hangzhou.aliyuncs.com

# 通义千问配置
ALIYUN_QWEN_API_KEY=your_qwen_api_key_here
ALIYUN_QWEN_MODEL=qwen-turbo

# ===================================
# 百度AI平台 (UNIT NLU)
# ===================================
# 申请地址: https://ai.baidu.com/
BAIDU_APP_ID=your_baidu_app_id_here
BAIDU_API_KEY=your_baidu_api_key_here
BAIDU_SECRET_KEY=your_baidu_secret_key_here

# 百度UNIT配置
BAIDU_UNIT_BOT_ID=your_unit_bot_id_here
BAIDU_UNIT_API_URL=https://aip.baidubce.com/rpc/2.0/unit/service/v3/chat

# ===================================
# 极光推送 (JPush)
# ===================================
# 申请地址: https://www.jiguang.cn/
JPUSH_APP_KEY=your_jpush_app_key_here
JPUSH_MASTER_SECRET=your_jpush_master_secret_here

# ===================================
# 高德地图 (定位服务)
# ===================================
# 申请地址: https://lbs.amap.com/
AMAP_KEY_ANDROID=your_amap_android_key_here
AMAP_KEY_IOS=your_amap_ios_key_here
AMAP_WEB_SERVICE_KEY=your_amap_web_service_key_here

# ===================================
# 短信服务配置 (可选)
# ===================================
SMS_PROVIDER=aliyun
SMS_ACCESS_KEY=your_sms_access_key_here
SMS_ACCESS_SECRET=your_sms_access_secret_here
SMS_SIGN_NAME=银龄守候
SMS_TEMPLATE_CODE_VERIFY=SMS_123456789

# ===================================
# 对象存储配置
# ===================================
OSS_PROVIDER=aliyun
OSS_ACCESS_KEY=your_oss_access_key_here
OSS_ACCESS_SECRET=your_oss_access_secret_here
OSS_BUCKET=yinling-guardian
OSS_ENDPOINT=oss-cn-hangzhou.aliyuncs.com
OSS_CDN_URL=https://cdn.yinling.com

# ===================================
# 监控配置
# ===================================
# Prometheus
PROMETHEUS_PORT=9090

# Grafana
GRAFANA_PORT=3000
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=your_grafana_password_here

# AlertManager
ALERTMANAGER_PORT=9093

# ===================================
# 日志配置
# ===================================
# Elasticsearch
ELASTICSEARCH_HOST=localhost
ELASTICSEARCH_PORT=9200
ELASTICSEARCH_USERNAME=elastic
ELASTICSEARCH_PASSWORD=your_elasticsearch_password_here

# Kibana
KIBANA_PORT=5601

# Logstash
LOGSTASH_PORT=5044

# ===================================
# 邮件配置 (用于告警通知)
# ===================================
MAIL_HOST=smtp.qq.com
MAIL_PORT=587
MAIL_USERNAME=your_email@qq.com
MAIL_PASSWORD=your_email_auth_code_here
MAIL_FROM=your_email@qq.com

# ===================================
# 钉钉/企业微信机器人 (告警通知)
# ===================================
DINGTALK_WEBHOOK=https://oapi.dingtalk.com/robot/send?access_token=your_token_here
WECHAT_WORK_WEBHOOK=https://qyapi.weixin.qq.com/cgi-bin/webhook/send?key=your_key_here

# ===================================
# Android应用配置
# ===================================
ANDROID_VERSION_NAME=1.0.0
ANDROID_VERSION_CODE=1
ANDROID_MIN_SDK_VERSION=26
ANDROID_TARGET_SDK_VERSION=34
ANDROID_COMPILE_SDK_VERSION=34

# ===================================
# Flutter应用配置
# ===================================
FLUTTER_VERSION_NAME=1.0.0
FLUTTER_VERSION_CODE=1

# ===================================
# 调试配置
# ===================================
# 是否开启调试模式
DEBUG_MODE=true

# 日志级别: TRACE/DEBUG/INFO/WARN/ERROR
LOG_LEVEL=INFO

# 是否打印SQL日志
SQL_LOG_ENABLED=true

# ===================================
# 性能配置
# ===================================
# 数据库连接池大小
DB_POOL_SIZE=20
DB_MAX_POOL_SIZE=50

# Redis连接池大小
REDIS_POOL_SIZE=20
REDIS_MAX_POOL_SIZE=50

# 线程池配置
THREAD_POOL_CORE_SIZE=10
THREAD_POOL_MAX_SIZE=50
THREAD_POOL_QUEUE_CAPACITY=100

# ===================================
# 安全配置
# ===================================
# AES加密密钥 (256位)
AES_ENCRYPTION_KEY=your_aes_256_bit_key_here_32_chars

# API签名密钥
API_SIGN_KEY=your_api_sign_key_here

# IP白名单 (逗号分隔)
IP_WHITELIST=127.0.0.1,::1

# 接口限流配置 (每秒请求数)
RATE_LIMIT_PER_SECOND=10

# ===================================
# 文件上传配置
# ===================================
# 文件上传最大大小 (MB)
FILE_UPLOAD_MAX_SIZE=10

# 图片上传最大大小 (MB)
IMAGE_UPLOAD_MAX_SIZE=5

# 语音文件最大大小 (MB)
AUDIO_UPLOAD_MAX_SIZE=20

# 视频文件最大大小 (MB)
VIDEO_UPLOAD_MAX_SIZE=100

# ===================================
# WebSocket配置
# ===================================
WEBSOCKET_PORT=8088
WEBSOCKET_PATH=/ws

# ===================================
# WebRTC配置
# ===================================
WEBRTC_TURN_SERVER=turn:your-turn-server.com:3478
WEBRTC_TURN_USERNAME=your_turn_username
WEBRTC_TURN_PASSWORD=your_turn_password

# ===================================
# 定时任务配置
# ===================================
# Cron表达式: 健康报告生成 (每天凌晨3点)
CRON_HEALTH_REPORT=0 0 3 * * ?

# Cron表达式: 用药提醒检查 (每小时)
CRON_MEDICATION_REMINDER=0 0 * * * ?

# Cron表达式: 数据备份 (每天凌晨2点)
CRON_DATA_BACKUP=0 0 2 * * ?

# ===================================
# 第三方服务超时配置 (毫秒)
# ===================================
ASR_TIMEOUT=5000
TTS_TIMEOUT=5000
OCR_TIMEOUT=3000
NLU_TIMEOUT=3000
LLM_TIMEOUT=10000

# ===================================
# 缓存过期时间配置 (秒)
# ===================================
CACHE_USER_INFO_TTL=3600
CACHE_HEALTH_DATA_TTL=1800
CACHE_AI_RESPONSE_TTL=7200
CACHE_HOT_DATA_TTL=300
